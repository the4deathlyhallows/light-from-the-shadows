from operator import truediv


def is_palindrome(text):

    length= len(text)

    for i in range(0, length //2):
        if (text[i] != text[length - i - 1]):
           return False
    return True

string1 = "racecar"
print(is_palindrome(string1))

string2 = "abceba"
print(is_palindrome(string2))




